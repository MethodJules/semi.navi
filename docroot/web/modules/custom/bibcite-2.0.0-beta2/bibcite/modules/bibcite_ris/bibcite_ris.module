<?php

/**
 * @file
 * Module hooks.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function bibcite_ris_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.bibcite_ris':
      $module = 'bibcite_ris';
      return \Drupal::service('bibcite.help_service')
        ->getHelpMarkup([], $route_name, $module);
  }
}
